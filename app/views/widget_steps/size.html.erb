<div class="span12">
	<div class="span8">
		<h1>Choose a size:</h1>
		<%= form_for @widget, url: wizard_path do |f| %>
		<div class="row size-step">
			<div class="med-rect"><p>300x250</p></div>
		</div>
		<div class="row size-step">
			<div class="rect"><p>180x150</p></div>
		</div>
		<div class="row size-step">
			<div class="wide-sky"><p>160x600</p></div>
		</div>
		<div class="row size-step">
			<div class="leaderboard"><p>728x90</p></div>
		</div>
		<% end %>
	</div>
</div>